{
    "name": "badtiger/yii2-file-kit",
    "description": "Yii2 file upload and storage kit",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Alexandr Konradi",
            "email": "alexandrkonradi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2-jui": "*",
        "league/flysystem": "*",
        "npm-asset/blueimp-file-upload": "*",
        "rmrevin/yii2-fontawesome": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5.0",
        "creocoder/yii2-flysystem": "~0.8"
    },
    "autoload": {
        "psr-4": { "badtiger\\filekit\\": "src/" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
